<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  id="Definitions_Sponsoring"
                  targetNamespace="http://bpmn.io/schema/bpmn"
                  exporter="Camunda Modeler"
                  exporterVersion="5.44.0"
                  camunda:diagramRelationId="diagram-1">

  <bpmn:message id="Message_Angebotsanforderung" name="MSG_ANGEBOTSANFORDERUNG" />

  <bpmn:process id="PROC_SPONSORING" name="Sponsoring Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>Flow_Start_Plan</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:userTask id="Task_Plan" name="Leistungs- und Kapazitätsplan erstellen">
      <bpmn:extensionElements>
        <camunda:formRef>plan_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Start_Plan</bpmn:incoming>
      <bpmn:outgoing>Flow_Plan_Preisliste</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_Preisliste" name="Preisliste erstellen">
      <bpmn:extensionElements>
        <camunda:formRef>preisliste_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Plan_Preisliste</bpmn:incoming>
      <bpmn:outgoing>Flow_Preisliste_Mappe</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_Mappe" name="Sponsorenmappe erstellen">
      <bpmn:extensionElements>
        <camunda:formRef>mappe_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Preisliste_Mappe</bpmn:incoming>
      <bpmn:outgoing>Flow_Mappe_EventGateway</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:eventBasedGateway id="Gateway_Eventbasiert" name="Warte auf Ereignis">
      <bpmn:incoming>Flow_Mappe_EventGateway</bpmn:incoming>
      <bpmn:outgoing>Flow_EventGateway_Timer</bpmn:outgoing>
      <bpmn:outgoing>Flow_EventGateway_Message</bpmn:outgoing>
    </bpmn:eventBasedGateway>

    <bpmn:intermediateCatchEvent id="Event_Timer" name="6 Monate vor der Veranstaltung">
      <bpmn:incoming>Flow_EventGateway_Timer</bpmn:incoming>
      <bpmn:outgoing>Flow_Timer_AbsagenDirekt</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef_1">
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT30S</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>

    <bpmn:intermediateCatchEvent id="Event_Angebotsanforderung" name="Angebotsanforderung">
      <bpmn:incoming>Flow_EventGateway_Message</bpmn:incoming>
      <bpmn:outgoing>Flow_Message_Angabotsanforderung</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageDef_Angebotsanforderung" messageRef="Message_Angebotsanforderung" />
    </bpmn:intermediateCatchEvent>

    <bpmn:userTask id="Task_Angabotsanforderung" name="Angabots anforderung erfassen">
      <bpmn:extensionElements>
        <camunda:formRef>angebotsanforderung_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Message_Angabotsanforderung</bpmn:incoming>
      <bpmn:incoming>Flow_DatenAendern_Back</bpmn:incoming>
      <bpmn:outgoing>Flow_Angabotsanforderung_DatenVollstaendig</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="Gateway_DatenVollstaendig" name="Daten vollständig?">
      <bpmn:incoming>Flow_Angabotsanforderung_DatenVollstaendig</bpmn:incoming>
      <bpmn:outgoing>Flow_DatenVollstaendig_Ja</bpmn:outgoing>
      <bpmn:outgoing>Flow_DatenVollstaendig_Nein</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:userTask id="Task_DatenAendern" name="daten Ändern">
      <bpmn:extensionElements>
        <camunda:formRef>daten_aendern_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_DatenVollstaendig_Nein</bpmn:incoming>
      <bpmn:outgoing>Flow_DatenAendern_Back</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="Gateway_0c9ecfi" name="Angebot möglich?">
      <bpmn:incoming>Flow_DatenVollstaendig_Ja</bpmn:incoming>
      <bpmn:outgoing>Flow_AngebotMoeglich_Ja</bpmn:outgoing>
      <bpmn:outgoing>Flow_AngebotMoeglich_Nein</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:userTask id="Task_AngebotSchreiben" name="Angebot schreiben">
      <bpmn:extensionElements>
        <camunda:formRef>angebot_schreiben_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AngebotMoeglich_Ja</bpmn:incoming>
      <bpmn:outgoing>Flow_AngebotSchreiben_Verschicken</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_AngebotVerschicken" name="Angebot verschicken">
      <bpmn:extensionElements>
        <camunda:formRef>angebot_verschicken_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AngebotSchreiben_Verschicken</bpmn:incoming>
      <bpmn:outgoing>Flow_Verschicken_EndeAngebot</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="End_AngebotVerschickt" name="Angebot verschickt">
      <bpmn:incoming>Flow_Verschicken_EndeAngebot</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:userTask id="Task_SponsorenAbsagen" name="Sponsoren absagen">
      <bpmn:extensionElements>
        <camunda:formRef>sponsoren_absagen_form</camunda:formRef>
        <camunda:formRefBinding>latest</camunda:formRefBinding>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AngebotMoeglich_Nein</bpmn:incoming>
      <bpmn:incoming>Flow_Timer_AbsagenDirekt</bpmn:incoming>
      <bpmn:outgoing>Flow_Absagen_EndeKeinSponsoring</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="End_KeinSponsoring" name="kein Sponsoring">
      <bpmn:incoming>Flow_Absagen_EndeKeinSponsoring</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Plan" sourceRef="StartEvent_1" targetRef="Task_Plan" />
    <bpmn:sequenceFlow id="Flow_Plan_Preisliste" sourceRef="Task_Plan" targetRef="Task_Preisliste" />
    <bpmn:sequenceFlow id="Flow_Preisliste_Mappe" sourceRef="Task_Preisliste" targetRef="Task_Mappe" />
    <bpmn:sequenceFlow id="Flow_Mappe_EventGateway" sourceRef="Task_Mappe" targetRef="Gateway_Eventbasiert" />

    <bpmn:sequenceFlow id="Flow_EventGateway_Timer" sourceRef="Gateway_Eventbasiert" targetRef="Event_Timer" />
    <bpmn:sequenceFlow id="Flow_Timer_AbsagenDirekt" sourceRef="Event_Timer" targetRef="Task_SponsorenAbsagen" />

    <bpmn:sequenceFlow id="Flow_EventGateway_Message" sourceRef="Gateway_Eventbasiert" targetRef="Event_Angebotsanforderung" />
    <bpmn:sequenceFlow id="Flow_Message_Angabotsanforderung" sourceRef="Event_Angebotsanforderung" targetRef="Task_Angabotsanforderung" />

    <bpmn:sequenceFlow id="Flow_Angabotsanforderung_DatenVollstaendig" sourceRef="Task_Angabotsanforderung" targetRef="Gateway_DatenVollstaendig" />

    <bpmn:sequenceFlow id="Flow_DatenVollstaendig_Ja" name="ja" sourceRef="Gateway_DatenVollstaendig" targetRef="Gateway_0c9ecfi">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${datenVollstaendig == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_DatenVollstaendig_Nein" name="nein" sourceRef="Gateway_DatenVollstaendig" targetRef="Task_DatenAendern">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${datenVollstaendig == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_DatenAendern_Back" sourceRef="Task_DatenAendern" targetRef="Task_Angabotsanforderung" />

    <bpmn:sequenceFlow id="Flow_AngebotMoeglich_Ja" name="ja" sourceRef="Gateway_0c9ecfi" targetRef="Task_AngebotSchreiben">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${angebotMoeglich == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_AngebotMoeglich_Nein" name="nein" sourceRef="Gateway_0c9ecfi" targetRef="Task_SponsorenAbsagen">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${angebotMoeglich == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_AngebotSchreiben_Verschicken" sourceRef="Task_AngebotSchreiben" targetRef="Task_AngebotVerschicken" />
    <bpmn:sequenceFlow id="Flow_Verschicken_EndeAngebot" sourceRef="Task_AngebotVerschicken" targetRef="End_AngebotVerschickt" />
    <bpmn:sequenceFlow id="Flow_Absagen_EndeKeinSponsoring" sourceRef="Task_SponsorenAbsagen" targetRef="End_KeinSponsoring" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="PROC_SPONSORING" />
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
